{
	"schemaVersion": 1,
	"id": "utility-toolkit",
	"version": "${version}",
	"name": "Utility Toolkit",
	"description": "ユーティリティMod",
	"authors": [
		"chihalu"
	],
	"contact": {},
	"license": "MIT",
	"icon": "assets/utility-toolkit/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"chihalu.building.support.BuildingSupport"
		],
		"client": [
			"chihalu.building.support.BuildingSupportClient"
		],
		"modmenu": [
			"chihalu.building.support.BuildingSupportModMenuIntegration"
		],
		"fabric-datagen": [
			"chihalu.building.support.BuildingSupportDataGenerator"
		]
	},
	"mixins": [
		"utility-toolkit.mixins.json",
		{
			"config": "utility-toolkit.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.9",
		"java": ">=21",
		"fabric-api": "*"
	}
