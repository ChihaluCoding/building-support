{
	"schemaVersion": 1,
	"id": "building-support",
	"version": "${version}",
	"name": "Building Support",
	"description": "Adds a Favorites tab to the creative inventory for quick access to frequently used blocks and items.",
	"authors": [
		"chihalu"
	],
	"contact": {},
	"license": "CC0-1.0",
	"icon": "assets/building-support/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"chihalu.building.support.BuildingSupport"
		],
		"client": [
			"chihalu.building.support.BuildingSupportClient"
		],
		"fabric-datagen": [
			"chihalu.building.support.BuildingSupportDataGenerator"
		]
	},
	"mixins": [
		"building-support.mixins.json",
		{
			"config": "building-support.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.3",
		"minecraft": "~1.21.9",
		"java": ">=21",
		"fabric-api": "*"
	}
}
